{
    "builds":[
        {
            "src": "website/main.py", 
            "use": "@vercel/python"
        }
    ],
    "routes":[
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
            "dest": "/$1"
        },
        {
            "src": "/(.*)", 
            "dest": "website/main.py"
        }
        
    ]
}