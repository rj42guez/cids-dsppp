{
    "functions": {
        "api/index.py": {
            "runtime": "python3.9",
            "excludeFiles": "public/**"
        }
    },
    "routes":[
        {
            "src": "/(.+\\.[a-z]+)$",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)", 
            "dest": "/api/index.py"
        }
        
    ]
}